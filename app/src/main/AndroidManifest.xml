<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joaquin.hackathon">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/latest_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/latest_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hackathon">
        <activity android:name="com.joaquin.hackathon.Activity.internetActivity" />
        <activity
            android:name="com.joaquin.hackathon.Activity.WhatsappActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.joaquin.hackathon.Activity.VideoPlayerActivity"
            android:screenOrientation="landscape" />
        <activity android:name="com.joaquin.hackathon.Activity.DownloadPopupActivity" />
        <activity android:name="com.joaquin.hackathon.Activity.MainDialogActivity" />
        <activity
            android:name="com.joaquin.hackathon.Activity.TabedActivity"
            android:screenOrientation="portrait"
            android:theme="@style/tab" />
        <activity
            android:name="com.joaquin.hackathon.Activity.AccountSettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.joaquin.hackathon.Activity.ForgetPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.joaquin.hackathon.Activity.SignUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.joaquin.hackathon.Activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.joaquin.hackathon.Activity.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.joaquin.hackathon.Activity.MainActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>